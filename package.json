{
	"name": "svelte-preprocess-css-mods",
	"type": "module",
	"version": "0.1.8",
	"packageManager": "pnpm@9.10.0",
	"description": "use css modules in svelte",
	"author": "ryoppippi",
	"license": "MIT",
	"repository": "ryoppippi/svelte-preprocess-css-mods",
	"keywords": [
		"svelte",
		"svlte-preprocess",
		"css",
		"css-modules"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"jsr": "./src/index.ts"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"LICENSE",
		"README.md",
		"dist",
		"package.json",
		"src"
	],
	"scripts": {
		"test": "vitest",
		"build": "mkdist -d",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"typecheck": "tsc --noEmit",
		"release": "nr release:pre && nlx bumpp && nr release:npm && nr release:jsr",
		"release:pre": "nr typecheck && nr test run && nr build && nlx publint",
		"release:npm": "npm publish",
		"release:jsr": "nlx pkg-to-jsr && nlx jsr publish --allow-dirty"
	},
	"dependencies": {
		"betterr": "^5.1.0",
		"camelcase-keys": "^9.1.3",
		"defu": "^6.1.4",
		"knitwork": "^1.1.0",
		"lightningcss": "^1.27.0",
		"magic-string": "^0.30.11",
		"mlly": "^1.7.1",
		"svelte": "5.0.0-next.245",
		"svelte-parse-markup": "^0.1.5",
		"type-fest": "^4.26.1",
		"ufo": "^1.5.4",
		"uint8array-extras": "^1.4.0",
		"unconfig": "^0.5.5",
		"zimmerframe": "^1.1.2"
	},
	"devDependencies": {
		"@antfu/ni": "^0.23.0",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config",
		"@sveltejs/kit": "^2.5.26",
		"@types/node": "^22.5.4",
		"eslint": "^9.10.0",
		"eslint-plugin-format": "^0.1.2",
		"eslint-plugin-svelte": "^2.43.0",
		"mkdist": "^1.5.9",
		"typescript": "^5.6.2",
		"vite": "^5.4.4",
		"vitest": "^2.0.5"
	}
}
